pipeline {
    agent any
    options {
        timestamps()
        ansiColor('xterm')
        timeout(time: 2, unit: 'MINUTES')
    }
    stages {
        stage("build") {
            steps {
                echo 'build'
            }
        }
        stage("run") {
            steps {
                echo 'run'
            }
        }
    }
}